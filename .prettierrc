{
    "arrowParens": "avoid",
    "useTabs": true,
    "singleQuote": true,
    "tabWidth": 2,
    "semi": false,
    "trailingComma": "none",
    "importOrder": [
        "<THIRD_PARTY_MODULES>",
        "^@/core/components/(.*)$",
        "^@/core/components/layout/(.*)$",
        "^@/core/components/screens/(.*)$",
        "^@/core/components/ui(.*)$",
        "^@/styles/(.*)$",
        "^@/config/(.*)$",
        "^@/utils/(.*)$",
        "^@/services/(.*)$",
        "^@/store/(.*)$",
        "^@/api/(.*)$",
        "^.+\\.(templates\\.html|module\\.scss",
        "^../(.*)",
        "^./(.*)"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
}